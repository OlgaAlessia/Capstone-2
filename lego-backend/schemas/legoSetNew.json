{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/legoSetNew.schema.json",
  "type": "object",
  "properties": {
    "set_num": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },
    "year": {
      "type": "integer",
      "minLength": 4,
      "maxLength": 4
    },
    "theme_id": {
      "type": "integer"
    },
    "num_parts": {
      "type": "integer",
      "minimum": 0
    },
    "set_img_url": {
      "type": "string",
      "format": "uri"
    },
    "link_instruction": {
      "type": "string",
      "format": "uri"
    }
  },
  "additionalProperties": false,
  "required": [
    "set_num",
    "name",
    "num_parts",
    "set_img_url"
  ]
}
